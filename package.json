{
  "name": "card-games",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --all --parallel",
    "build": "cross-env NODE_ENV=production nx run-many --target=build --all --prod",
    "test": "nx run-many --target=test --all",
    "lint": "nx workspace-lint && nx run-many --target=lint --all --max-warnings 0",
    "e2e": "nx run-many --target=e2e --all",
    "format": "nx format:write",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "autoprefixer": "10.3.1",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "postcss": "8.3.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "regenerator-runtime": "0.13.7",
    "tailwindcss": "2.2.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@nrwl/cli": "12.6.3",
    "@nrwl/cypress": "12.6.3",
    "@nrwl/eslint-plugin-nx": "12.6.3",
    "@nrwl/jest": "12.6.3",
    "@nrwl/linter": "12.6.3",
    "@nrwl/react": "12.6.3",
    "@nrwl/tao": "12.6.3",
    "@nrwl/web": "12.6.3",
    "@nrwl/workspace": "12.6.3",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "babel-jest": "27.0.6",
    "cross-env": "7.0.3",
    "cypress": "^7.3.0",
    "dotenv": "~10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.1",
    "jest": "27.0.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,json,css,scss,md,ts,tsx,html}": [
      "npm run format -- --uncommitted",
      "npm run lint"
    ]
  }
}
